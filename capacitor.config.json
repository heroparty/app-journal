{
  "appId": "com.unisul.journaling-my-journey",
  "appName": "Journaling my journey",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    }
  },
  "cordova": {},
  "android": {
    "overrideUserAgent": "jmj-custom-agent",
    "appendUserAgent": "jmj-custom-append",
    "backgroundColor": "#0B88DB",
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": true,
    "hideLogs": false
  }
}